<!DOCTYPE html>
<html>
<body>

<p>Click the button to join two arrays.</p>

<button onclick="myFunction()">Try it</button>

<p id="demo"></p>

<script>
function myFunction() {
  var hege = ["Cecilie", "Lone"];
  var stale = ["Emil", "Tobias", "Linus"];
  var children = hege.concat(stale); 
  var zombies = ["zombie1", "zombie2", "zombie3"]
  var extended = children.concat(zombies);
  document.getElementById("demo").innerHTML = extended;
}
</script>

</body>
</html>
